#Задание 5:
n = int(input("введите натуральное число n: "))
v = 0
if n < 0:
    print(Error)
else:
    for i in range(1, n + 1):
        v += i**3
    print('Ответ на задание 5:', v)

# Задание 10:
n = int(input('введите количество чисел из ряда Фибаначи(n): '))
k = int(input('введите порядковый номер в ряду, с которого нужно начать(k): '))
count = 0
number1 = 0
number2 = 1
number_fib = 0
sum = 0
for count in range(n):
    if count < k-1:
            number_fib = number1
            number1 = number2
            number2 += number_fib
            count += 1
            continue
    
    number_fib = number1
    number1 = number2
    number2 += number_fib
    count += 1
    sum += number_fib
print ('ответ на задание 10:', sum)
